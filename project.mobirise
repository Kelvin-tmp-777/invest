          "_name": "accordion01",
          "_sourceTheme": "carm4",
          "_customHTML": "<section class=\"accordion1 carm4_accordion1\" group=\"Accordions\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{'mbr-parallax-background':bg.parallax, 'mbr-fullscreen': fullScreen}\" plugins=\"mbr-switch-arrow2, mobirise-icons2\">\n\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"9\" step=\"1\" value=\"6\" condition=\"fullScreen == false\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"9\" step=\"1\" value=\"5\" condition=\"fullScreen == false\">\n        <select title=\"Accordions Count\" name=\"bootstrapAccordion\">\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\">4</option>\n            <option value=\"5\" selected>5</option>\n            <option value=\"6\">6</option>\n        </select>\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showsubTitle\">\n        <input type=\"checkbox\" title=\"Text\" name=\"showText\">\n        <input type=\"checkbox\" title=\"Column title\" name=\"showColumnTitle\">\n        <header condition=\"showTitle\">Mask</header>\n        <input type=\"checkbox\" title=\"Title Mask\" name=\"showTitleMask\" condition=\"showTitle\" checked>\n        <input type=\"color\" title=\"Mask Text Color\" name=\"maskTextColor\" value=\"#ffffff\" selected condition=\"showTitleMask && showTitle\">\n        <input type=\"checkbox\" title=\"Gradient Mask\" name=\"gradientMask\" condition=\"showTitleMask && showTitle\" checked>\n        <input type=\"color\" title=\"Mask Color\" name=\"maskColor\" value=\"#ff5e3a\" condition=\"showTitleMask && showTitle\">\n        <input type=\"color\" title=\"Mask Color 2\" name=\"maskColor2\" value=\"#ff653c\" condition=\"gradientMask && showTitleMask && showTitle\">\n        <input type=\"color\" title=\"Mask Color 3\" name=\"maskColor3\" value=\"#ff4e3c\" condition=\"gradientMask && showTitleMask && showTitle\">\n        <header>Color</header>\n        <input type=\"color\" name=\"cardColor\" title=\"Card Bg\" value=\"#f5f5f5\" selected>\n        <input type=\"color\" name=\"iconColor\" title=\"Icons\" value=\"#ff4e3c\" selected>\n        <input type=\"color\" name=\"lineColor\" title=\"Line\" value=\"#efefef\" selected>\n        <input type=\"color\" name=\"openLineColor\" title=\"Open Line\" value=\"#ff4e3c\" selected>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Background Image\" value=\"../_images/background1.jpg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#ffffff\" selected>\n            <input type=\"video\" title=\"Background Video\" value=\"http://www.youtube.com/watch?v=uNCr7NdOJgw\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" checked condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.9\" condition=\"overlay && bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <div mbr-class=\"{'container-fluid': fullWidth, 'container': !fullWidth }\">\n        <div class=\"row\">\n            <div class=\"col-12 col-md-12 title-text\">\n                <h3 class=\"mbr-section-title mbr-bold align-center mbr-fonts-style\" data-app-selector=\".title-text .mbr-section-title\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\">\n                    Вопросы&nbsp;<span>Ответы</span></h3>\n                <h4 class=\"mbr-section-subtitle pb-1 align-center mbr-light mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showsubTitle\" data-app-selector=\".title-text .mbr-section-subtitle\">The\n                    Car\n                    Repair Appointment</h4>\n                <p class=\"mbr-text mbr-regular align-center mbr-fonts-style mt-4\" mbr-theme-style=\"display-4\" mbr-if=\"showText\" data-app-selector=\".title-text .mbr-text\">Lorem ipsum dolor sit amet,\n                    consectetur\n                    adipisicing elit, sed do\n                    eiusmod tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam, quis nostru\n                    exercitation ullamco laboris nisi ut aliquip ex lorem ipsum.</p>\n            </div>\n        </div>\n        <div class=\"row\">\n            <div class=\"col-lg-12\">\n                <div id=\"bootstrap-accordion\" class=\"panel-group accordionStyles accordion\" role=\"tablist\" aria-multiselectable=\"true\">\n                    <div class=\"row\">\n                        <div class=\"col-lg-6\">\n                            <h3 class=\"mbr-section-subtitle align-left mbr-black mbr-regular pb-0 pb-lg-4 mbr-fonts-style second-subtitle\" mbr-if=\"showColumnTitle\" mbr-theme-style=\"display-7\" data-app-selector=\".second-subtitle\">\n                                Personalization options</h3>\n                            <div class=\"card\">\n                                <div class=\"card-header\" role=\"tab\" id=\"headingOne\">\n                                    <a role=\"button\" class=\"panel-title collapsed text-black pr-3\" data-toggle=\"collapse\" data-core href=\"#collapse1\" aria-expanded=\"false\" aria-controls=\"collapse1\">\n\n                                        <h4 class=\"mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card .card-header a.panel-title h4\">\n                                            Страхуется ли товар?\n                                            <span class=\"toggle-icon inactive sign mbr-iconfont\"></span>\n                                        </h4>\n\n                                    </a>\n                                </div>\n                                <div id=\"collapse1\" class=\"panel-collapse noScroll collapse \" role=\"tabpanel\" aria-labelledby=\"headingOne\" data-parent=\"#accordion\">\n                                    <div class=\"panel-body\">\n                                        <p class=\"mbr-fonts-style panel-text pr-2\" mbr-theme-style=\"display-4\" data-app-selector=\".card .panel-body p\">\n                                            Да, страхуем весь товарный остаток и помещение хранения товаров.</p>\n                                    </div>\n                                </div>\n                            </div>\n                            <div class=\"card\" mbr-if=\"bootstrapAccordion > 1\">\n                                <div class=\"card-header\" role=\"tab\" id=\"headingTwo\">\n                                    <a role=\"button\" class=\"collapsed panel-title text-black pr-3\" data-toggle=\"collapse\" data-core href=\"#collapse2\" aria-expanded=\"false\" aria-controls=\"collapse2\">\n                                        <h4 class=\" mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card .card-header a.panel-title h4\"><span class=\"toggle-icon inactive sign mbr-iconfont\"></span>Как быстро продается весь машинокомплект?\n                                        </h4>\n                                    </a>\n\n                                </div>\n                                <div id=\"collapse2\" class=\"panel-collapse noScroll collapse\" role=\"tabpanel\" aria-labelledby=\"headingTwo\" data-parent=\"#accordion\">\n                                    <div class=\"panel-body\">\n                                        <p class=\"mbr-fonts-style panel-text pr-2\" mbr-theme-style=\"display-4\" data-app-selector=\".card .panel-body pt\">За два месяца машинокомплект продается на 90%, за счет этого быстрая окупаемость средств.</p>\n                                    </div>\n                                </div>\n                            </div>\n                            <div class=\"card\" mbr-if=\"bootstrapAccordion > 2\">\n                                <div class=\"card-header\" role=\"tab\" id=\"headingThree\">\n                                    <a role=\"button\" class=\"collapsed text-black panel-title pr-3\" data-toggle=\"collapse\" data-core href=\"#collapse3\" aria-expanded=\"false\" aria-controls=\"collapse3\">\n                                        <h4 class=\" mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card .card-header a.panel-title h4\"><span class=\"toggle-icon inactive sign mbr-iconfont\"></span>Как оформляется договор?</h4>\n                                    </a>\n                                </div>\n                                <div id=\"collapse3\" class=\"panel-collapse noScroll collapse\" role=\"tabpanel\" aria-labelledby=\"headingThree\" data-parent=\"#accordion\">\n                                    <div class=\"panel-body\">\n                                        <p class=\"mbr-fonts-style panel-text pr-2\" mbr-theme-style=\"display-4\" data-app-selector=\".card .panel-body p\">Оформляем договор займа с поручительством. \n<br>Вы можете заключить договор:\n<br>- с ООО как ИП\n<br>- С ООО как ООО\n<br>- С физ. лицом и физ. лицом<br>Налоги во всех случаях оплачивает инвестор самостоятельно.</p>\n                                    </div>\n                                </div>\n                            </div>\n                            <div class=\"card\" mbr-if=\"bootstrapAccordion > 3\">\n                                <div class=\"card-header\" role=\"tab\" id=\"headingFour\">\n                                    <a role=\"button\" class=\"collapsed panel-title text-black pr-3\" data-toggle=\"collapse\" data-core href=\"#collapse4\" aria-expanded=\"false\" aria-controls=\"collapse4\">\n                                        <h4 class=\" mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card .card-header a.panel-title h4\"><span class=\"toggle-icon inactive sign mbr-iconfont\"></span>\n                                            Какие гарантии?</h4>\n                                    </a>\n                                </div>\n                                <div id=\"collapse4\" class=\"panel-collapse noScroll collapse\" role=\"tabpanel\" aria-labelledby=\"headingFour\" data-parent=\"#accordion\">\n                                    <div class=\"panel-body\">\n                                        <p class=\"mbr-fonts-style panel-text pr-2\" mbr-theme-style=\"display-4\" data-app-selector=\".card .panel-body p\">Гарантия это наш бизнес и товарный остаток. Все активы подтверждаются и прописываются в договоре. Помимо всего у нас есть еще успешный бизнес в IT сфере, который также выступает в роли гаранта.</p>\n                                    </div>\n                                </div>\n                            </div>\n                            <div class=\"card\" mbr-if=\"bootstrapAccordion > 4\">\n                                <div class=\"card-header\" role=\"tab\" id=\"headingFive\">\n                                    <a role=\"button\" class=\"collapsed panel-title text-black pr-3\" data-toggle=\"collapse\" data-core href=\"#collapse5\" aria-expanded=\"false\" aria-controls=\"collapse5\">\n                                        <h4 class=\" mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card .card-header a.panel-title h4\"><span class=\"toggle-icon inactive sign mbr-iconfont\"></span>Можно контролировать процесс?\n                                        </h4>\n                                    </a>\n                                </div>\n                                <div id=\"collapse5\" class=\"panel-collapse noScroll collapse\" role=\"tabpanel\" aria-labelledby=\"headingFive\" data-parent=\"#accordion\">\n                                    <div class=\"panel-body\">\n                                        <p class=\"mbr-fonts-style panel-text pr-2\" mbr-theme-style=\"display-4\" data-app-selector=\".card .panel-body p\">\n                                            Да, будет предоставлен личный кабинет в котором можно видеть отчеты и как быстро окупаются машинокомплекты.</p>\n                                    </div>\n                                </div>\n                            </div>\n                            <div class=\"card\" mbr-if=\"bootstrapAccordion > 5\">\n                                <div class=\"card-header\" role=\"tab\" id=\"headingSix\">\n                                    <a role=\"button\" class=\"collapsed panel-title text-black pr-3\" data-toggle=\"collapse\" data-core href=\"#collapse6\" aria-expanded=\"false\" aria-controls=\"collapse6\">\n                                        <h4 class=\" mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card .card-header a.panel-title h4\"><span class=\"toggle-icon inactive sign mbr-iconfont\"></span>\n                                            Lacinia quis vel eros donec ac odio tempor orci\n                                        </h4>\n                                    </a>\n                                </div>\n                                <div id=\"collapse6\" class=\"panel-collapse noScroll collapse\" role=\"tabpanel\" aria-labelledby=\"headingSix\" data-parent=\"#accordion\">\n                                    <div class=\"panel-body\">\n                                        <p class=\"mbr-fonts-style panel-text pr-2\" mbr-theme-style=\"display-4\" data-app-selector=\".card .panel-body pt\">\n                                            Aliquam purus sit amet luctus venenatis lectus magna fringilla urna. Egestas\n                                            congue quisque egestas diam in arcu cursus euismod.\n                                        </p>\n                                    </div>\n                                </div>\n                            </div>\n                        </div>\n                        <div class=\"col-lg-6\">\n                            <h3 class=\"mbr-section-subtitle align-left mbr-black mbr-regular pb-0 pb-lg-4 mbr-fonts-style second-subtitle\" mbr-if=\"showColumnTitle\" mbr-theme-style=\"display-7\" data-app-selector=\".second-subtitle\">Social\n                                media integration</h3>\n                            <div class=\"card\">\n                                <div class=\"card-header\" role=\"tab\" id=\"headingOne\">\n                                    <a role=\"button\" class=\"panel-title collapsed text-black pr-3\" data-toggle=\"collapse\" data-core href=\"#collapse7\" aria-expanded=\"false\" aria-controls=\"collapse7\">\n                                        <h4 class=\" mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card .card-header a.panel-title h4\"><span class=\"toggle-icon inactive sign mbr-iconfont\"></span>На что пойдут мои деньги?</h4>\n                                    </a>\n                                </div>\n                                <div id=\"collapse7\" class=\"panel-collapse noScroll collapse \" role=\"tabpanel\" aria-labelledby=\"headingOne\" data-parent=\"#accordion\">\n                                    <div class=\"panel-body\">\n                                        <p class=\"mbr-fonts-style panel-text pr-2\" mbr-theme-style=\"display-4\" data-app-selector=\".card .panel-body p\">\n                                            Вы получаете полный отчет по закупленным машинокомплектам и реализации автозапчастей.</p>\n                                    </div>\n                                </div>\n                            </div>\n                            <div class=\"card\" mbr-if=\"bootstrapAccordion > 1\">\n                                <div class=\"card-header\" role=\"tab\" id=\"headingTwo\">\n                                    <a role=\"button\" class=\"collapsed panel-title text-black pr-3\" data-toggle=\"collapse\" data-core href=\"#collapse8\" aria-expanded=\"false\" aria-controls=\"collapse8\">\n                                        <h4 class=\" mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card .card-header a.panel-title h4\"><span class=\"toggle-icon inactive sign mbr-iconfont\"></span>В чем ваше конкурентное преимущество на рынке?</h4>\n                                    </a>\n\n                                </div>\n                                <div id=\"collapse8\" class=\"panel-collapse noScroll collapse\" role=\"tabpanel\" aria-labelledby=\"headingTwo\" data-parent=\"#accordion\">\n                                    <div class=\"panel-body\">\n                                        <p class=\"mbr-fonts-style panel-text pr-2\" mbr-theme-style=\"display-4\" data-app-selector=\".card .panel-body p\">\n                                            Для роста на уже сформировавшемся рынке не нужно придумывать ноу-хау. Клиенты уже знают что хотят.<br>Поэтому наше преимущество перед конкурентами в обработке их запросов по трем составляющим:<br>1) Скорость подбора командой экспертов. Всегда есть что предложить покупателям с точным подбором по применимости в кротчайшие сроки.<br>2) Удержание выгодной цены без потери качества товара.<br>3) Предоставляем гарантию на все товары. Для клиентов безусловный возврат денег при возврате запчасти.</p>\n                                    </div>\n                                </div>\n                            </div>\n                            <div class=\"card\" mbr-if=\"bootstrapAccordion > 2\">\n                                <div class=\"card-header\" role=\"tab\" id=\"headingThree\">\n                                    <a role=\"button\" class=\"collapsed text-black panel-title pr-3\" data-toggle=\"collapse\" data-core href=\"#collapse9\" aria-expanded=\"false\" aria-controls=\"collapse9\">\n                                        <h4 class=\" mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card .card-header a.panel-title h4\"><span class=\"toggle-icon inactive sign mbr-iconfont\"></span>Могу ли рассчитывать на скидки?</h4>\n                                    </a>\n                                </div>\n                                <div id=\"collapse9\" class=\"panel-collapse noScroll collapse\" role=\"tabpanel\" aria-labelledby=\"headingThree\" data-parent=\"#accordion\">\n                                    <div class=\"panel-body\">\n                                        <p class=\"mbr-fonts-style panel-text pr-2\" mbr-theme-style=\"display-4\" data-app-selector=\".card .panel-body p\">Для наших инвесторов закрепляется скидка в размере 30% на автозапчасти.</p>\n                                    </div>\n                                </div>\n                            </div>\n                            <div class=\"card\" mbr-if=\"bootstrapAccordion > 3\">\n                                <div class=\"card-header\" role=\"tab\" id=\"headingFour\">\n                                    <a role=\"button\" class=\"collapsed panel-title text-black pr-3\" data-toggle=\"collapse\" data-core href=\"#collapse10\" aria-expanded=\"false\" aria-controls=\"collapse10\">\n                                        <h4 class=\" mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card .card-header a.panel-title h4\"><span class=\"toggle-icon inactive sign mbr-iconfont\"></span>\n                                            Почему мне стоит инвестировать в ваш бизнес?</h4>\n                                    </a>\n                                </div>\n                                <div id=\"collapse10\" class=\"panel-collapse noScroll collapse\" role=\"tabpanel\" aria-labelledby=\"headingFour\" data-parent=\"#accordion\">\n                                    <div class=\"panel-body\">\n                                        <p class=\"mbr-fonts-style panel-text pr-2\" mbr-theme-style=\"display-4\" data-app-selector=\".card .panel-body p\">1) Опираясь на наш опыт с 2011 года, мы создали собственную платформу в которой объединили авторазборы и конечного покупателя.&nbsp;<br>2) У нас есть четкая стратегия на ближайшие 5 лет, включающая перспективы роста и расширения проекта, направления инвестиций, ожидаемые продажи и прибыли.<br>3)&nbsp;Инвестирование в наш бизнес защитит ваши средства от инфляции и проложит путь к быстрому росту на развивающемся рынке.</p>\n                                    </div>\n                                </div>\n                            </div>\n                            <div class=\"card\" mbr-if=\"bootstrapAccordion > 4\">\n                                <div class=\"card-header\" role=\"tab\" id=\"headingFive\">\n                                    <a role=\"button\" class=\"collapsed panel-title text-black pr-3\" data-toggle=\"collapse\" data-core href=\"#collapse11\" aria-expanded=\"false\" aria-controls=\"collapse11\">\n                                        <h4 class=\" mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card .card-header a.panel-title h4\"><span class=\"toggle-icon inactive sign mbr-iconfont\"></span>Могу ли я встретиться с собственником бизнеса?</h4>\n                                    </a>\n                                </div>\n                                <div id=\"collapse11\" class=\"panel-collapse noScroll collapse\" role=\"tabpanel\" aria-labelledby=\"headingFive\" data-parent=\"#accordion\">\n                                    <div class=\"panel-body\">\n                                        <p class=\"mbr-fonts-style panel-text pr-2\" mbr-theme-style=\"display-4\" data-app-selector=\".card .panel-body p\">Безусловно, будем рады личной встречи и с удовольствием покажем как все устроено.</p>\n                                    </div>\n                                </div>\n                            </div>\n                            <div class=\"card\" mbr-if=\"bootstrapAccordion > 5\">\n                                <div class=\"card-header\" role=\"tab\" id=\"headingSix\">\n                                    <a role=\"button\" class=\"collapsed panel-title text-black pr-3\" data-toggle=\"collapse\" data-core href=\"#collapse12\" aria-expanded=\"false\" aria-controls=\"collapse12\">\n                                        <h4 class=\" mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".card .card-header a.panel-title h4\"><span class=\"toggle-icon inactive sign mbr-iconfont\"></span>\n                                            Non blandit massa enim nec dui nunc mattis\n                                        </h4>\n                                    </a>\n                                </div>\n                                <div id=\"collapse12\" class=\"panel-collapse noScroll collapse\" role=\"tabpanel\" aria-labelledby=\"headingSix\" data-parent=\"#accordion\">\n                                    <div class=\"panel-body\">\n                                        <p class=\"mbr-fonts-style panel-text pr-2\" mbr-theme-style=\"display-4\" data-app-selector=\".card .panel-body p\">\n                                            Aliquam purus sit amet luctus venenatis lectus magna fringilla urna. Egestas\n                                            congue quisque egestas diam in arcu cursus euismod.</p>\n                                    </div>\n                                </div>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
          "_cid": "tCi3S6kuJm",
          "_anchor": "accordion01-e",
          "_protectedParams": [],
          "_global": false,
          "_once": false,
          "_params": {}
        },
        {
          "alias": false,
          "_styles": {
            "& when not (@fullScreen)": {
              "padding-top": "(@paddingTop * 15px)",
              "padding-bottom": "(@paddingBottom * 15px)"
            },
            "& when (@bg-type = \"color\")": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = \"image\")": {
              "background-image": "url(@bg-value)"
            },
            "& when (@reverseContent)": {
              ".row": {
                "flex-direction": "row-reverse"
              }
            },
            ".mbr-section-title": {
              "color": "#06182d",
              "& when (@showTitleMask)": {
                "span": {
                  "line-height": "1",
                  "color": "@maskTextColor",
                  "padding": "0 6px 1px 5px",
                  "transform": "skew(-14deg)",
                  "background": "@maskColor",
                  "& when (@gradientMask)": {
                    "background": "linear-gradient(@maskColor 0%, @maskColor2 69%, @maskColor3 100%)"
                  },
                  "display": "inline-block",
                  "font-weight": "500",
                  "letter-spacing": "0",
                  "position": "relative"
                }
              }
            },
            ".mbr-section-subtitle": {
              "color": "#06182d",
              "&.display-7": {
                "letter-spacing": "0"
              }
            },
            ".mbr-section-text": {
              "color": "#999999"
            },
            ".mbr-iconfont": {
              "font-size": "48px",
              "padding-right": "1rem"
            },
            ".form-control, .form-check-label": {
              "font-size": "16px",
              "font-weight": "300",
              "line-height": "1"
            },
            "& when (@customStyle)": {
              ".form-control": {
                "box-shadow": "none",
                "margin-bottom": "15px",
                "padding": "0.7em 1.07em 0em 0.8em",
                "background-color": "@fieldColor",
                "border": "0px solid @borderColor",
                "border-bottom": "1px solid @borderColor",
                "color": "@formText",
                "line-height": "1.43",
                "min-height": "3.5em",
                "border-radius": "0.25rem",
                "&:focus, &:hover": {
                  "border": "0px solid @borderHover",
                  "background-color": "@fieldHover"
                },
                "&.textarea": {
                  "padding-top": "40px"
                }
              },
              ".form-group": {
                "margin-bottom": "1rem",
                ".wrap": {
                  "display": "flex",
                  "span": {
                    "padding": "0em 0em",
                    "margin-bottom": "0px",
                    "align-self": "center",
                    "color": "@formPlaceholder"
                  }
                }
              },
              "input::-webkit-input-placeholder, textarea::-webkit-input-placeholder": {
                "color": "@formPlaceholder"
              },
              "input:-moz-placeholder, textarea:-moz-placeholder": {
                "color": "@formPlaceholder"
              },
              ".jq-selectbox li, .jq-selectbox li": {
                "background-color": "@fieldColor",
                "color": "contrast(@fieldColor, #000000, #ffffff, 30%)"
              },
              ".jq-selectbox li:hover, .jq-selectbox li.selected": {
                "background-color": "@fieldHover",
                "color": "contrast(@fieldHover, #000000, #ffffff, 30%)"
              },
              ".jq-selectbox:hover .jq-selectbox__trigger-arrow": {
                "border-top-color": "@fieldColor"
              },
              ".jq-selectbox .jq-selectbox__trigger-arrow": {
                "border-top-color": "@fieldHover"
              }
            },
            "& when not (@customStyle)": {
              ".form-group": {
                ".wrap": {
                  "span": {
                    "display": "none"
                  }
                }
              }
            },
            ".map": {
              "width": "100%",
              "height": "30rem",
              "iframe": {
                "width": "inherit",
                "height": "100%"
              }
            },
            ".icon-block": {
              "margin-bottom": "10px",
              "display": "flex",
              "align-items": "center",
              ".icon-block__icon": {
                "display": "inline-block",
                "flex-shrink": "0"
              },
              ".icon-block__title": {
                "display": "inline-block",
